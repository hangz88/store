
a = ["北京","上海","广东"]
a.append("郑州")
print(a)
a[1] = "广东"
print(a)

num=[36710.36,35427.10,29863.23,29667.39,27665.36,27650.45,27620.38,25369.20]
all_num = (num[0] + num[1] + num[2] + num[3] + num[4] + num[5] + num[6] + num[7])
print("总值:" , all_num)   #总值
pl_num = (all_num / 8)
print("平均值:" , pl_num)   # 平均值